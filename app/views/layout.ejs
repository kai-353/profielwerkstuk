<% if(typeof user != 'undefined') { %>
        <li class="nav-item dropdown" style="list-style-type: none;">
          <a class="btn dropdown-toggle"
          id="nav-dropdown" 
          href="#" id="navbarDropdownMenuLink" 
          data-toggle="dropdown"
          aria-haspopup="true" 
          aria-expanded="false">
            <%= user.username %> 
          </a>
          <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
            <a class="dropdown-item" href="/users/profile/<%= user.id %>">Profile</a>
            <a class="dropdown-item" href="/users/favorites">Favorites</a>
            <a class="dropdown-item" href="/users/logout">Log out</a>
          </div>
        </li>
<% } else { %>
      <a class="nav-link" href="/users/register/" id="normal-nav-button">Register</a>
      <a
        class="btn me-3"
        href="/users/login"
        id="nav-login"
        >Login</a
      >
<% }; %>
</div>
</nav>
